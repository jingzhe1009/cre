<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bonc.frame.mapper.oracle.auth.ChannelMapper">

    <resultMap id="BaseResultMap" type="com.bonc.frame.entity.auth.Channel">
        <id column="CHANNEL_ID" property="channelId" jdbcType="VARCHAR"/>
        <result column="CHANNEL_NAME" property="channelName" jdbcType="VARCHAR"/>
        <result column="CHANNEL_DESC" property="channelDesc" jdbcType="VARCHAR"/>
        <result column="CHANNEL_CODE" property="channelCode" jdbcType="VARCHAR"/>
        <result column="PARENT_ID" property="parentId" jdbcType="VARCHAR"/>
        <result column="DEPT_ID" property="deptId" jdbcType="VARCHAR"/>
        <result column="CREATE_DATE" property="createDate" jdbcType="TIMESTAMP"/>
        <result column="UPDATE_DATE" property="updateDate" jdbcType="TIMESTAMP"/>
        <result column="CREATE_PERSON" property="createPerson" jdbcType="VARCHAR"/>
        <result column="UPDATE_PERSON" property="updatePerson" jdbcType="VARCHAR"/>
    </resultMap>

    <resultMap id="BaseResultMap2" extends="BaseResultMap" type="com.bonc.frame.entity.auth.ChannelExt">
        <result column="PARENT_NAME" property="parentName" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
      CHANNEL_ID,CHANNEL_NAME, CHANNEL_DESC,CHANNEL_CODE, PARENT_ID, DEPT_ID,CREATE_DATE, UPDATE_DATE, CREATE_PERSON,UPDATE_PERSON
    </sql>

    <sql id="Base_Column_List2">
      a.CHANNEL_ID,
      a.CHANNEL_NAME,
      a.CHANNEL_DESC,
      a.PARENT_ID,
      a.DEPT_ID,
      a.USER_NUM,
      a.CREATE_DATE,
      a.UPDATE_DATE,
      a.CREATE_PERSON,
      a.UPDATE_PERSON
    </sql>


    <insert id="insert" parameterType="com.bonc.frame.entity.auth.Channel">
        insert into CRE_AUTH_CHANNEL
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="channelId != null">
                CHANNEL_ID,
            </if>
            <if test="channelName != null">
                CHANNEL_NAME,
            </if>
            <if test="channelDesc != null">
                CHANNEL_DESC,
            </if>
            <if test="parentId != null">
                PARENT_ID,
            </if>
            <if test="deptId != null">
                DEPT_ID,
            </if>
            <if test="userNum != null">
                USER_NUM,
            </if>
            <if test="createDate != null">
                CREATE_DATE,
            </if>
            <if test="updateDate != null">
                UPDATE_DATE,
            </if>
            <if test="createPerson != null">
                CREATE_PERSON,
            </if>
            <if test="updatePerson != null">
                UPDATE_PERSON,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="channelId != null">
                #{channelId,jdbcType=VARCHAR},
            </if>
            <if test="channelName != null">
                #{channelName,jdbcType=VARCHAR},
            </if>
            <if test="channelDesc != null">
                #{channelDesc,jdbcType=VARCHAR},
            </if>
            <if test="parentId != null">
                #{parentId,jdbcType=VARCHAR},
            </if>
            <if test="deptId != null">
                #{deptId,jdbcType=VARCHAR},
            </if>
            <if test="userNum != null">
                #{userNum,jdbcType=INTEGER},
            </if>
            <if test="createDate != null">
                #{createDate,jdbcType=TIMESTAMP},
            </if>
            <if test="updateDate != null">
                #{updateDate,jdbcType=TIMESTAMP},
            </if>
            <if test="createPerson != null">
                #{createPerson,jdbcType=VARCHAR},
            </if>
            <if test="updatePerson != null">
                #{updatePerson,jdbcType=VARCHAR},
            </if>
        </trim>

    </insert>

    <select id="listByChannelName" resultMap="BaseResultMap2" parameterType="map">
        select
        <include refid="Base_Column_List2"/>, b.CHANNEL_NAME as "PARENT_NAME"
        from
        CRE_AUTH_CHANNEL a
        left join CRE_AUTH_CHANNEL b
        on a.PARENT_ID = b.CHANNEL_ID
        where 1=1
        <if test="channelName != null and '' != channelName">
            <bind name="channelNamePattern" value="'%' + _parameter.channelName + '%'"/>
            and a.CHANNEL_NAME like #{channelNamePattern}
        </if>
    </select>

    <select id="list" resultMap="BaseResultMap" >
        select
        <include refid="Base_Column_List"/>
        from CRE_AUTH_CHANNEL
    </select>

    <select id="selectByPrimaryKey" resultMap="BaseResultMap" parameterType="java.lang.String">
        select
        <include refid="Base_Column_List"/>
        from CRE_AUTH_CHANNEL
        where CHANNEL_ID = #{channelId,jdbcType=VARCHAR}
    </select>

    <select id="findByName" resultMap="BaseResultMap" parameterType="java.lang.String">
        select
        <include refid="Base_Column_List"/>
        from CRE_AUTH_CHANNEL
        where CHANNEL_NAME = #{channelName,jdbcType=VARCHAR}
    </select>
    <select id="nameList" resultType="map">
        select
        CHANNEL_ID,CHANNEL_NAME
        from CRE_AUTH_CHANNEL
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.String">
        delete from CRE_AUTH_CHANNEL
        where CHANNEL_ID = #{channelId,jdbcType=VARCHAR} or PARENT_ID = #{channelId,jdbcType=VARCHAR}
    </delete>

    <update id="updateByPrimaryKeySelective" parameterType="com.bonc.frame.entity.auth.Channel">
        update CRE_AUTH_CHANNEL
        <set>
            <if test="channelId != null">
                CHANNEL_ID=#{channelId,jdbcType=VARCHAR},
            </if>
            <if test="channelName != null">
                CHANNEL_NAME=#{channelName,jdbcType=VARCHAR},
            </if>
            <if test="channelDesc != null">
                CHANNEL_DESC=#{channelDesc,jdbcType=VARCHAR},
            </if>
            <if test="parentId != null">
                PARENT_ID= #{parentId,jdbcType=VARCHAR},
            </if>
            <if test="deptId != null">
                DEPT_ID= #{deptId,jdbcType=VARCHAR},
            </if>
            <if test="createDate != null">
                CREATE_DATE=#{createDate,jdbcType=TIMESTAMP},
            </if>
            <if test="updateDate != null">
                UPDATE_DATE=#{updateDate,jdbcType=TIMESTAMP},
            </if>
            <if test="createPerson != null">
                CREATE_PERSON=#{createPerson,jdbcType=VARCHAR},
            </if>
            <if test="updatePerson != null">
                UPDATE_PERSON=#{updatePerson,jdbcType=VARCHAR},
            </if>
        </set>
        where CHANNEL_ID = #{channelId,jdbcType=VARCHAR}
    </update>

    <select id="getChannelByDept" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from CRE_AUTH_CHANNEL
        where DEPT_ID = #{deptId,jdbcType=VARCHAR}
    </select>

    <select id="channelListByDept" parameterType="java.lang.String" resultType="map">
        select
            CHANNEL_ID,CHANNEL_NAME
        from CRE_AUTH_CHANNEL
        where DEPT_ID = #{deptId,jdbcType=VARCHAR}
        order by CREATE_DATE desc
    </select>

    <select id="getUserByChannel" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        ID, USER_ID, CHAN_ID
        from CRE_AUTH_USER_CHAN
        where CHAN_ID = #{channelId,jdbcType=VARCHAR}
    </select>

    <select id="userListByChannel" parameterType="java.lang.String" resultType="map">
        select
            a.USER_ID, b.USER_NAME,
        from CRE_AUTH_USER_CHAN a
        left join CRE_AUTH_USER u on u.ID = a.USER_ID
        where a.CHAN_ID = #{channelId,jdbcType=VARCHAR}
        order by a.ID desc
    </select>


</mapper>